<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.text.Font?>

<VBox scaleShape="false"
      stylesheets="/resources/style.css"
      xmlns="http://javafx.com/javafx/8"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="Loader.AccountsListController">
    <Label prefWidth="Infinity" alignment="CENTER" text="Your current accounts on Skype">
        <font>
            <Font size="24.0" />
        </font>
    </Label>
    <GridPane fx:id="grid">
        <columnConstraints>
            <ColumnConstraints />
            <ColumnConstraints />
            <ColumnConstraints minWidth="48" maxWidth="48"/>
            <ColumnConstraints minWidth="164" maxWidth="164"/>
            <ColumnConstraints />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="32.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="32.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="32.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="32.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="32.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="32.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="32.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="32.0" vgrow="SOMETIMES"/>
        </rowConstraints>

        <padding>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
        </padding>
    </GridPane>
</VBox>

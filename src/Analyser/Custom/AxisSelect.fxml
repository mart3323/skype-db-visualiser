<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Analyser.Custom.AxisController">
    <children>

        <Label text="Select axis"> <font> <Font size="36.0" /> </font> </Label>
        <Label fx:id="user_name" text="Custom scatter plot" />
      <VBox alignment="CENTER" fillWidth="false">
         <children>
      
                  <HBox alignment="CENTER">
               <children>
                  <Label text="X axis: " />
                  <ChoiceBox fx:id="XSelect" />
               </children>
            </HBox>
            <HBox alignment="CENTER">
               <children>
                  <Label text="Y axis: " />
                  <ChoiceBox fx:id="YSelect" />
               </children>
            </HBox>
         </children>
         <VBox.margin>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </VBox.margin>
      </VBox>
      <AnchorPane>
         <children>
            <Button disable="true" mnemonicParsing="false" text="Back" />
            <Button defaultButton="true" mnemonicParsing="false" text="Next" onAction="#next" AnchorPane.rightAnchor="0.0" />
         </children>
      </AnchorPane>
    </children>

     <padding> <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" /> </padding>
 </VBox>
